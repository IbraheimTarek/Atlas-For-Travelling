            <div class="composition">
              <img src="/img/nat-1-large.jpg " alt="p1" class="composition__photo composition__photo--p1">
              <img src="/img/nat-2-large.jpg" alt="p2" class="composition__photo composition__photo--p2">
              <img src="/img/nat-3-large.jpg" alt="p3" class="composition__photo composition__photo--p3">
            </div>
          </div>
        </div>
      </section>
      <section class="section-feature">
        <div class="Row">
          <div class="cal-1-of-4">
            <div class="feature-box">
              <i class="feature-box__icon fa-solid fa-globe"></i>
              <h3 class="heading-tertiary u-margin-bottom-small ">explore the world</h3>
              <p class="paragraph">lorem ipsum dolor sit amet consectetur adipisicing elit. Aperiam, ipsum sapiente aspernatur</p>
            </div>
          </div>
          <div class="cal-1-of-4">
            <div class="feature-box">
              <i class="feature-box__icon fa-regular fa-compass"></i>
              <h3 class="heading-tertiary u-margin-bottom-small ">MEET NATURE</h3>
              <p class="paragraph">lorem ipsum dolor sit amet consectetur adipisicing elit. Aperiam, ipsum sapiente aspernatur</p>
            </div>
          </div>
          <div class="cal-1-of-4">
            <div class="feature-box">
              <i class="feature-box__icon fa-regular fa-map"></i>
              <h3 class="heading-tertiary u-margin-bottom-small ">FIND YOUR WAY</h3>
              <p class="paragraph">lorem ipsum dolor sit amet consectetur adipisicing elit. Aperiam, ipsum sapiente aspernatur</p>
            </div>
          </div>
          <div class="cal-1-of-4">
            <div class="feature-box">
              <i class="feature-box__icon fa-regular fa-heart"></i>
              <h3 class="heading-tertiary u-margin-bottom-small ">LIVE A HEALTHIER LIFE</h3>
              <p class="paragraph">lorem ipsum dolor sit amet consectetur adipisicing elit. Aperiam, ipsum sapiente aspernatur</p>
            </div>
          </div>
        </div>
      </section>
        <!-- start contact -->
        <div class="contact" id="contact"style="background-color: black;">
          <div class="container">
              <h2 class="special-heading" >Contact</h2>
              <p>We are born to create</p>
              <div class="text" >
                  <p style="color: #ffc107;"> Feel free to drop us a line at:</p>
                  <a href="mailto:Leonagency@maiLocom" class="link" style="color: #ffc107;">Atlas@mail.com</a>
                  <div class="social"style="color:whitesmoke;">
                      <span style="color: #ffc107;">Find Us On Social Networks</span>
                      <i class="fa-brands fa-youtube" style="color: #ffc107;"></i>
                      <i class="fa-brands fa-facebook" style="color: #ffc107;"></i>
                      <i class="fa-brands fa-twitter" style="color: #ffc107;"></i>
                  </div>
              </div>
          </div>
      </div>
      <!-- end contact -->
      <!-- Start Footer -->
      <div class="footer" style="color: black; background-color: #ffc107;" >&copy; 2021 <span>Atlas</span> All Right Reserved</div>
      <!-- End Footer -->


