<% layout("boilerplate") %>

<div class="container mt-5 mb-5">
  <form
    action="/places?_method=put"
    method="post"
  >
    <div class="mb-3">
      <label for="placeName" class="form-label">Name</label>
      <input
      name="name"
        type="text"
        class="form-control"
        id="placeName"
        aria-describedby="emailHelp"
         placeholder="old"
      />
    </div>
    <div class="mb-3">
      <label for="countryName" class="form-label">Country Name</label>
      <input name="country_name" type="text" class="form-control" id="countryName" placeholder="old" required/>
    </div>
    <div class="mb-3">
      <label for="longitude" class="form-label">Longitude</label>
      <input name="longitude" type="text" class="form-control" id="Longitude"  placeholder="old"required/>
    </div>
    <div class="mb-3">
      <label for="latitude" class="form-label">Latitude</label>
      <input name="latitude" type="text" class="form-control" id="latitude"  placeholder="old"required/>
    </div>
    <div class="mb-3">
      <label for="population" class="form-label">Population</label>
      <input name="population" type="text" class="form-control" id="Population"  placeholder="old"required/>
    </div>
    <div class="mb-3">
      <label for="history" class="form-label">history</label>
      <input name="history" type="text" class="form-control" id="history"  placeholder="old"required/>
    </div>
    <div class="mb-3">
      <label for="religion" class="form-label">Religion</label>
      <input name="religion" type="text" class="form-control" id="religion"  placeholder="old"required/>
    </div>
    <div class="mb-3">
      <label for="photoURL" class="form-label">Photo URL</label>
      <input name="photoURL" type="text" class="form-control" id="photoURL"  placeholder="old"required/>
    </div>
    <input type="hidden" id="placeType" name="placeType" value="0">
    <button type="submit" class="btn btn-success btn-insert">Insert</button>
  </form>
</div>
